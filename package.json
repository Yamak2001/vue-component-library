{
  "name": "vue-component-library",
  "version": "0.1.0",
  "private": false,
  "description": "A Vue 3 component library with three-tier architecture",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-component-library.umd.js",
  "module": "./dist/vue-component-library.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-component-library.es.js",
      "require": "./dist/vue-component-library.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "sideEffects": [
    "*.vue",
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "peerDependencies": {
    "pinia": "^2.1.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.42.0",
    "jsdom": "^22.1.0",
    "path": "^0.12.7",
    "postcss": "^8.4.24",
    "rollup": "^3.25.1",
    "sass": "^1.63.3",
    "typescript": "^5.0.4",
    "url": "^0.11.4",
    "vite": "^4.5.13",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  },
  "dependencies": {
    "pinia": "^2.1.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui-library",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vue-component-library.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vue-component-library/issues"
  },
  "homepage": "https://github.com/yourusername/vue-component-library#readme"
}
